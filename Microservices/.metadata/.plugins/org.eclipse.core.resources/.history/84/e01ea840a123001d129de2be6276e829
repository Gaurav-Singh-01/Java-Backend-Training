package com.globallogic.employee.services;

import java.util.List;

import org.springframework.stereotype.Service;

import com.globallogic.employee.entity.EmployeeEntity;


@Service
public interface EmployeeEntityService {
	
	
	public List<EmployeeEntity> showEmployees();
	
	public List<EmployeeEntity> addEmployees();
	
	public List<EmployeeEntity> updateEmployees();
	
	public List<EmployeeEntity> deleteEmployees();

}
