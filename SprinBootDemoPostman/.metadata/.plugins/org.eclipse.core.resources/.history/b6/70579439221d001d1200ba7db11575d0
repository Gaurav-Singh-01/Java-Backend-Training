package com.assignments.Ecommerce.entity;

import javax.persistence.*;
import javax.persistence.Table;

@Entity
@Table(name = "merchant")
public class Merchant {
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	int id;
	
	@Column
	String name;
	
	@Column
	long pinCode;
	
	@Column
	String location;
	
	@Column
	long gstNo;

}
